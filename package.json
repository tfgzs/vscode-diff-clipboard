{
  "name": "diff-clipboard",
  "displayName": "Diff Clipboard",
  "description": "compare with clipboard 剪切板代码对比",
  "version": "0.0.1",
  "publisher": "tfgzs",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "logo.png",
  "homepage": "https://github.com/tfgzs/vscode-diff-clipboard",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfgzs/vscode-diff-clipboard"
  },
  "bugs": {
    "url": "https://github.com/tfgzs/vscode-diff-clipboard/issues"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "diff",
    "clipboard",
    "text",
    "string"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "diffCode.compareWithClipboard",
        "title": "%diffCode.compareWithClipboard.title%"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "diffCode.compareWithClipboard",
          "title": "%diffCode.compareWithClipboard.title%"
        }
      ]
    }
  },
  "scripts": {
    "build": "node build.js",
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "esbuild": "^0.19.9",
    "eslint": "^8.54.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "typescript": "^5.3.2"
  }
}